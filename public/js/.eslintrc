{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": false
  },
  "rules": {
    "arrow-spacing": [ 1, {"before": false,"after": true}],
    "comma-dangle": [1, "never"],
    "object-curly-spacing": [1, "never"],
    "import/no-unresolved": 0
  }
}
